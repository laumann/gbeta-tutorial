-- universe:descriptor --
{
  Account: %{
    balance: @int;
    
    Deposit: %(amount:int|balance) {
      balance+amount|balance
    };
    
    Withdraw: %(amount:int|balance) {
      balance-amount|balance
    }
  };
  Ac1,Ac2,Ac3: @Account;
  k1,k2,k3: @int
#
  // L1
  100|Ac1.Deposit|k1;
  200|Ac2.Deposit|k2;
  300|Ac3.Deposit|k3;
  // L2
  150|Ac1.Deposit|k1;
  90|Ac3.Withdraw|k3;
  90|Ac2.Deposit|k2;
  80|Ac3.Withdraw|k3
  // L3
}
